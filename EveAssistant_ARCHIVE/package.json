{
  "name": "eveassistant",
  "productName": "EVE Assistant",
  "version": "1.0.0",
  "description": "An EVE Online fitting and skill planning assistant.",
  "author": "EveProjects",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint . --ext .ts,.tsx",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d src/main/db/dataSource.ts",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:e2e": "jest -c jest.config.playwright.js",
    "coverage": "jest --coverage --testPathIgnorePatterns=e2e",
    "security": "npm audit",
    "pre-e2e": "electron-forge package",
    "e2e": "playwright test",
    "test:api-mocks": "jest src/**/*.test.ts --testMatch='**/*.(api|esi|auth|sde)*.test.ts' --testPathIgnorePatterns=e2e",
    "test:e2e:api-mocks": "npx playwright test e2e/api-mocking.test.ts",
    "build:prod": "cross-env NODE_ENV=production electron-forge package",
    "make:prod": "cross-env NODE_ENV=production electron-forge make",
    "analyze": "cross-env ANALYZE=true electron-forge start",
    "migration:revert": "npm run typeorm migration:revert",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repo.git"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron-forge/shared-types": "^7.8.1",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^28.1.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "msw": "^2.9.0",
    "node-loader": "^2.1.0",
    "nodemon": "^3.1.10",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.1",
    "@mui/x-tree-view": "^8.5.1",
    "@sentry/electron": "^6.7.0",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^4.17.22",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/react-window": "^1.8.8",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "axios-retry": "^4.0.0",
    "dotenv": "^16.5.0",
    "electron-log": "^5.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.1.7",
    "express": "^5.1.0",
    "fast-xml-parser": "^5.2.3",
    "fs-extra": "^11.3.0",
    "fuse.js": "^7.1.0",
    "geneticalgorithm": "^1.0.2",
    "immer": "^10.1.1",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "keytar": "^7.9.0",
    "knex": "^3.1.0",
    "node-cache": "^5.1.2",
    "p-limit": "^5.0.0",
    "react": "^19.1.0",
    "react-chrono": "^2.4.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "react-window": "^1.8.11",
    "recharts": "^2.15.3",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "config": {
    "forge": "./forge.config.ts"
  },
  "build": {
    "appId": "com.electron.eve-assistant",
    "productName": "EVE Assistant",
    "files": [
      "./.webpack/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github"
    }
  }
}
